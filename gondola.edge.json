{
    "tests": "./built/**/*_test.js",
    "timeout": 10000,
    "output": "./results",
    "helpers": {
        "WebDriver": {
            "url": "https://demo.gondolatest.com",
            "browser": "MicrosoftEdge",
            "windowSize": "maximize",            
            "restart": true,
            "keepBrowserState": false,
            "keepCookies": false,
            "waitForTimeout": 10000,
            "smartWait": 5000,
            "timeouts": {
                "script": 60000,
                "pageLoad": 10000
            },
            "desiredCapabilities": {
                "chromeOptions": {
                    "args": [
                        "--disable-gpu", "--no-sandbox"
                    ]
                }
            }
        },
        "Mochawesome": {}
    },
    "include": {},
    "bootstrap": false,
    "plugins": {
        "wdio": {
            "services": [
                "selenium-standalone"
            ],
            "seleniumArgs": {
                "javaArgs": ["-Dwebdriver.edge.driver=F:/softwares/webdrivers/edgedriver_84/msedgedriver.exe"]
            },
            "enabled": true
        }
    },
    "mocha": {
        "reporterOptions": {
            "reportDir": "result/GeneralMills/EdgeChromium",
            "inlineAssets": true,
            "timestamp": "isoDateTime",
            "reportFilename": "[EdgeChromium]GeneralMills_demo_report",
            "autoOpen": true,
            "reportPageTitle": "[EdgeChromium] GeneralMills_demo_report"
        }
    },
    "name": "ts_example_chrome"
}